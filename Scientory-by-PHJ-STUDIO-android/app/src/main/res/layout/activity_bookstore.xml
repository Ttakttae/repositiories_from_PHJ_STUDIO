<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mainactivity.Bookstore">


    <LinearLayout
        android:id="@+id/linearLayout9"
        android:layout_below="@id/linearLayout11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:background="#FFFFFF"
            android:src="@drawable/yes24"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:textColor="#000000"
            android:text="yes24 연간도서 순위"
            android:layout_gravity="center"
            android:textSize="23dp" />

    </LinearLayout>

    <Button
        android:id="@+id/btn5"
        android:layout_below="@id/linearLayout11"
        android:layout_width="match_parent"
        android:background="#00FF0000"
        android:layout_height="100dp" />

    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_below="@id/linearLayout13"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:background="#FFFFFF"
            android:src="@drawable/kyobo"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:text="교보문고 연간도서 순위"
            android:textColor="#000000"
            android:layout_gravity="center"
            android:textSize="23dp" />

    </LinearLayout>

    <Button
        android:id="@+id/btn6"
        android:layout_below="@id/linearLayout13"
        android:layout_width="match_parent"
        android:background="#00FF0000"
        android:layout_height="100dp" />

    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:background="#FFFFFF"
            android:src="@drawable/yeongpung"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:textColor="#000000"
            android:text="영풍문고 연간도서 순위"
            android:layout_gravity="center"
            android:textSize="23dp" />

    </LinearLayout>

    <Button
        android:id="@+id/btn7"
        android:layout_width="match_parent"
        android:background="#00FF0000"
        android:layout_height="100dp" />

    <LinearLayout
        android:id="@+id/linearLayout12"
        android:layout_below="@id/linearLayout9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:background="#FFFFFF"
            android:src="@drawable/aladin"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:text="알라딘 연간도서 순위"
            android:textColor="#000000"
            android:layout_gravity="center"
            android:textSize="23dp" />

    </LinearLayout>

    <Button
        android:id="@+id/btn8"
        android:layout_below="@id/linearLayout9"
        android:layout_width="match_parent"
        android:background="#00FF0000"
        android:layout_height="100dp" />

    <LinearLayout
        android:id="@+id/linearLayout13"
        android:layout_below="@id/linearLayout12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="wrap_content"
            android:background="#FFFFFF"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:src="@drawable/interpark"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:id="@+id/ad2"
            android:configChanges="uiMode"
            android:text="인터파크 연간도서 순위"
            android:textColor="#000000"
            android:layout_gravity="center"
            android:textSize="23dp" />

    </LinearLayout>

    <Button
        android:id="@+id/btn9"
        android:layout_below="@id/linearLayout12"
        android:layout_width="match_parent"
        android:background="#00FF0000"
        android:layout_height="100dp" />

</RelativeLayout>